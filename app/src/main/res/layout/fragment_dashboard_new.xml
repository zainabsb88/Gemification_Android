<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <GridView
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:background="@color/yellow"
        android:padding="10dp"
        android:numColumns="3"
        android:id="@+id/gridView5"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/textView67"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F1F1F2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/textView67"
        android:id="@+id/relativeLayout3">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView32"
            android:src="@drawable/temppic"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tiago Santos"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/green"
            android:id="@+id/textView69"
            android:layout_below="@+id/imageView32"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_marginTop="8dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="C C Coloumbo"
            android:gravity="center"
            android:id="@+id/textView70"
            android:layout_below="@+id/textView69"
            android:layout_marginTop="8dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Photography"
            android:gravity="center"
            android:id="@+id/textView71"
            android:layout_below="@+id/textView70"
            android:layout_marginTop="5dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="C V 547"
            android:gravity="center"
            android:id="@+id/textView72"
            android:layout_below="@+id/textView71"
            android:layout_alignLeft="@+id/textView71"
            android:layout_alignStart="@+id/textView71"
            android:layout_marginTop="5dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/imageButton2"
            android:background="@drawable/flag"
            android:layout_alignBottom="@+id/imageView32"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="57dp"
            android:layout_marginStart="57dp"
            android:layout_marginBottom="30dp" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/imageButton3"
            android:background="@drawable/chat"
            android:layout_alignTop="@+id/imageButton2"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="57dp"
            android:layout_marginEnd="57dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/gridView5"
        android:layout_below="@+id/relativeLayout3">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView68"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:padding="10dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_above="@+id/textView68">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Badges"
                android:id="@+id/textView73"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageView33"
                android:layout_alignRight="@+id/textView73"
                android:layout_alignEnd="@+id/textView73"
                android:src="@mipmap/ic_tf1"
                android:layout_below="@+id/textView73"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/textView73"
                android:layout_alignStart="@+id/textView73"
                android:padding="5dp" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageView34"
                android:src="@mipmap/ic_tf3"
                android:padding="5dp"
                android:layout_below="@+id/textView73"
                android:layout_toRightOf="@+id/textView73"
                android:layout_alignBottom="@+id/imageView33"
                android:layout_marginLeft="5dp" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageView35"
                android:src="@mipmap/ic_tf2"
                android:padding="5dp"
                android:layout_below="@+id/textView73"
                android:layout_toRightOf="@+id/imageView34"
                android:layout_toEndOf="@+id/imageView34" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageView36"
                android:src="@mipmap/ic_tf4"
                android:padding="5dp"
                android:layout_alignTop="@+id/imageView35"
                android:layout_toRightOf="@+id/imageView35"
                android:layout_toEndOf="@+id/imageView35" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageView37"
                android:src="@mipmap/ic_tf5"
                android:padding="5dp"
                android:layout_below="@+id/textView73"
                android:layout_toRightOf="@+id/imageView36"
                android:layout_toEndOf="@+id/imageView36" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageView38"
                android:src="@mipmap/ic_tf6"
                android:padding="5dp"
                android:layout_below="@+id/textView73"
                android:layout_toRightOf="@+id/imageView37"
                android:layout_toEndOf="@+id/imageView37" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageView39"
                android:src="@mipmap/ic_tf7"
                android:padding="5dp"
                android:layout_below="@+id/textView73"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:padding="10dp"
            android:id="@+id/relative_levels"
            android:layout_alignTop="@+id/textView68">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Levels"
                android:id="@+id/textView74"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Fan"
                android:textColor="@color/green"
                android:id="@+id/textView75"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Figurant"
                android:textColor="@color/green"
                android:id="@+id/textView76"
                android:layout_alignTop="@+id/textView75"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <ProgressBar
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="90dp"
                android:layout_height="20dp"
                android:padding="1dp"
                android:max="100"
                android:progress="98"
                android:progressDrawable="@drawable/progress_custom_dashboard"
                android:indeterminateOnly="false"
                android:id="@+id/progressBar2"
                android:layout_below="@+id/textView75"
                android:layout_alignLeft="@+id/textView75"
                android:layout_alignStart="@+id/textView75"
                android:layout_marginTop="10dp" />

            <ProgressBar
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="90dp"
                android:layout_height="20dp"
                android:padding="1dp"
                android:max="100"
                android:progress="98"
                android:progressDrawable="@drawable/progress_custom_dashboard_rect"
                android:indeterminateOnly="false"
                android:id="@+id/progressBar3"
                android:layout_alignTop="@+id/progressBar2"
                android:layout_toRightOf="@+id/progressBar2"
                android:layout_toEndOf="@+id/progressBar2" />

            <ProgressBar
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="90dp"
                android:layout_height="20dp"
                android:padding="1dp"
                android:max="0"
                android:progress="98"
                android:progressDrawable="@drawable/progress_custom_dashboard_rect"
                android:indeterminateOnly="false"
                android:id="@+id/progressBar4"
                android:layout_alignTop="@+id/progressBar3"
                android:layout_toRightOf="@+id/progressBar3"
                android:layout_toEndOf="@+id/progressBar3" />

            <ProgressBar
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="90dp"
                android:layout_height="20dp"
                android:padding="1dp"
                android:max="0"
                android:progress="98"
                android:progressDrawable="@drawable/progress_custom_dashboard_right"
                android:indeterminateOnly="false"
                android:id="@+id/progressBar5"
                android:layout_alignTop="@+id/progressBar4"
                android:layout_toRightOf="@+id/progressBar4"
                android:layout_toEndOf="@+id/progressBar4" />

        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>